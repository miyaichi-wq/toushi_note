<div class="card my-3">
  <div class="card-body">
    <div class="row text-center">
        <div class="col-md-3">
          <%= user_stock.stock.name %>
        </div>
        <div class="col-md-3">
          <%= user_stock.price %><br>
          <%= user_stock.volume %>
        </div>
        <div class="col-md-3">
          15.0
        </div>
        <div class="col-md-3">
          <%= user_stock.volume * 15.0 %>
        </div>
    </div>
  </div>
</div>
